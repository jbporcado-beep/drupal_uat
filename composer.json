{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "noli42/chosen",
                "version": "3.0.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/noli42/chosen/releases/download/3.0.0/chosen-assets-v3.0.0.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^2.3",
        "defuse/php-encryption": "^2.4",
        "drupal/address": "^2.0",
        "drupal/better_exposed_filters": "^7.0",
        "drupal/bootstrap_barrio": "^5.5",
        "drupal/bootstrap_sass": "^5.0",
        "drupal/chosen": "^5.0",
        "drupal/chosen_lib": "^5.0",
        "drupal/core-composer-scaffold": "^11.2",
        "drupal/core-project-message": "^11.2",
        "drupal/core-recipe-unpack": "^11.2",
        "drupal/core-recommended": "^11.2",
        "drupal/csv_serialization": "^4.0",
        "drupal/devel": "^5.4",
        "drupal/diff": "^1.9",
        "drupal/disable_html5_validation": "^2.0",
        "drupal/dynamic_entity_reference": "^3.2",
        "drupal/encrypt": "^3.2",
        "drupal/flood_control": "^3.0",
        "drupal/jquery_ui": "^1.7",
        "drupal/jquery_ui_draggable": "^2.1",
        "drupal/jquery_ui_resizable": "^2.1",
        "drupal/json_field": "^1.5",
        "drupal/key": "^1.20",
        "drupal/key_value_field": "^1.4",
        "drupal/login_history": "^2.0",
        "drupal/login_security": "^2.0",
        "drupal/password_policy": "^4.0",
        "drupal/password_policy_character_types": "^4.0",
        "drupal/password_policy_characters": "^4.0",
        "drupal/password_policy_extras": "^4.0",
        "drupal/password_policy_history": "^4.0",
        "drupal/password_policy_length": "^4.0",
        "drupal/password_policy_username": "^4.0",
        "drupal/pdf_using_mpdf": "^3.0",
        "drupal/smtp": "^1.4",
        "drupal/structure_sync": "^2.0",
        "drupal/tfa": "^1.12",
        "drupal/tfa_email_otp": "1.0.0-rc1",
        "drupal/twig_tweak": "^3.4",
        "drupal/user_redirect": "^2.0",
        "drupal/view_password": "^6.0",
        "drupal/views_core_entity_reference": "1.0.0-alpha3",
        "drupal/views_data_export": "^1.6",
        "drush/drush": "^13.6",
        "mpdf/mpdf": "^8.2",
        "noli42/chosen": "3.0.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "symfony/dotenv": "^7.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/core-recipe-unpack": true,
            "oomphinc/composer-installers-extender": true,
            "php-http/discovery": true,
            "php-tuf/composer-integration": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "recipes/{$name}": [
                "type:drupal-recipe"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    },
    "require-dev": {
        "behat/mink-selenium2-driver": "^1.7",
        "drupal/core-dev": "^11.2",
        "phpunit/phpunit": "^11.5"
    }
}
